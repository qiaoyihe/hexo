#footer-wrap
  if theme.footer.owner.enable
    - var now = new Date()
    - var nowYear = now.getFullYear()
    if theme.footer.owner.since && theme.footer.owner.since != nowYear
      .copyright!= `&copy;${theme.footer.owner.since} - ${nowYear} By <a href="https://status.yyisq.com/" >${config.author}</a>`
    else
      .copyright!= `&copy;${nowYear} By <a href="https://status.yyisq.com/" >${config.author}</a>`
  if theme.footer.copyright
    .framework-info

  if theme.footer.custom_text
    .footer_custom_text!=`${theme.footer.custom_text}`
